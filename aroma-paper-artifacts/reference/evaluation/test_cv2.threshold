#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root cv2.threshold/snippets.json cv2.threshold"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root cv2.threshold/test.json cv2.threshold/test.py"
time python3 src/main/python/similar.py -c cv2.threshold/snippets.json -d cv2.threshold/tmpout
time python3 src/main/python/similar.py -d cv2.threshold/tmpout -f cv2.threshold/test.json -o cv2.threshold/output.txt
