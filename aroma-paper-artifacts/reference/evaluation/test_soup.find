#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root soup.find/snippets.json soup.find"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root soup.find/test.json soup.find/test.py"
time python3 src/main/python/similar.py -c soup.find/snippets.json -d soup.find/tmpout
time python3 src/main/python/similar.py -d soup.find/tmpout -f soup.find/test.json -o soup.find/output.txt
