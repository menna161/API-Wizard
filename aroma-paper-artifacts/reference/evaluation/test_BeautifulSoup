#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root BeautifulSoup/snippets.json BeautifulSoup"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root BeautifulSoup/test.json BeautifulSoup/test.py"
time python3 src/main/python/similar.py -c BeautifulSoup/snippets.json -d BeautifulSoup/tmpout
time python3 src/main/python/similar.py -d BeautifulSoup/tmpout -f BeautifulSoup/test.json -o BeautifulSoup/output.txt
