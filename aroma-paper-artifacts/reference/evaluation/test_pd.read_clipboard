#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root pd.read_clipboard/snippets.json pd.read_clipboard"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root pd.read_clipboard/test.json pd.read_clipboard/test.py"
time python3 src/main/python/similar.py -c pd.read_clipboard/snippets.json -d pd.read_clipboard/tmpout
time python3 src/main/python/similar.py -d pd.read_clipboard/tmpout -f pd.read_clipboard/test.json -o pd.read_clipboard/output.txt
