#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root train_test_split/snippets.json train_test_split"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root train_test_split/test.json train_test_split/test.py"
time python3 src/main/python/similar.py -c train_test_split/snippets.json -d train_test_split/tmpout
time python3 src/main/python/similar.py -d train_test_split/tmpout -f train_test_split/test.json -o train_test_split/output.txt
