#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root nn.Sequential/snippets.json nn.Sequential"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root nn.Sequential/test.json nn.Sequential/test.py"
time python3 src/main/python/similar.py -c nn.Sequential/snippets.json -d nn.Sequential/tmpout
time python3 src/main/python/similar.py -d nn.Sequential/tmpout -f nn.Sequential/test.json -o nn.Sequential/output.txt
