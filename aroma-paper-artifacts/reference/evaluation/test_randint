#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root randint/snippets.json randint"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root randint/test.json randint/test.py"
time python3 src/main/python/similar.py -c randint/snippets.json -d randint/tmpout
time python3 src/main/python/similar.py -d randint/tmpout -f randint/test.json -o randint/output.txt
