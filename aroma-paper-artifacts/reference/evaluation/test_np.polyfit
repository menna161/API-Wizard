#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root np.polyfit/snippets.json np.polyfit"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root np.polyfit/test.json np.polyfit/test.py"
time python3 src/main/python/similar.py -c np.polyfit/snippets.json -d np.polyfit/tmpout
time python3 src/main/python/similar.py -d np.polyfit/tmpout -f np.polyfit/test.json -o np.polyfit/output.txt
