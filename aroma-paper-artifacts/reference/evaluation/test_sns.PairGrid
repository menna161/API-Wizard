#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root sns.PairGrid/snippets.json sns.PairGrid"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root sns.PairGrid/test.json sns.PairGrid/test.py"
time python3 src/main/python/similar.py -c sns.PairGrid/snippets.json -d sns.PairGrid/tmpout
time python3 src/main/python/similar.py -d sns.PairGrid/tmpout -f sns.PairGrid/test.json -o sns.PairGrid/output.txt
