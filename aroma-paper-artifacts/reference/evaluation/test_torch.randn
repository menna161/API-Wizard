#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root torch.randn/snippets.json torch.randn"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root torch.randn/test.json torch.randn/test.py"
time python3 src/main/python/similar.py -c torch.randn/snippets.json -d torch.randn/tmpout
time python3 src/main/python/similar.py -d torch.randn/tmpout -f torch.randn/test.json -o torch.randn/output.txt
