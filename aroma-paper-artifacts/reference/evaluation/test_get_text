#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root get_text/snippets.json get_text"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root get_text/test.json get_text/test.py"
time python3 src/main/python/similar.py -c get_text/snippets.json -d get_text/tmpout
time python3 src/main/python/similar.py -d get_text/tmpout -f get_text/test.json -o get_text/output.txt
