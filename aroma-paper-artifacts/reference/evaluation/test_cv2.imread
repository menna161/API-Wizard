#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root cv2.imread/snippets.json cv2.imread"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root cv2.imread/test.json cv2.imread/test.py"
time python3 src/main/python/similar.py -c cv2.imread/snippets.json -d cv2.imread/tmpout
time python3 src/main/python/similar.py -d cv2.imread/tmpout -f cv2.imread/test.json -o cv2.imread/output.txt
