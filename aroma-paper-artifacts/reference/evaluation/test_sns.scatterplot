#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root sns.scatterplot/snippets.json sns.scatterplot"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root sns.scatterplot/test.json sns.scatterplot/test.py"
time python3 src/main/python/similar.py -c sns.scatterplot/snippets.json -d sns.scatterplot/tmpout
time python3 src/main/python/similar.py -d sns.scatterplot/tmpout -f sns.scatterplot/test.json -o sns.scatterplot/output.txt
