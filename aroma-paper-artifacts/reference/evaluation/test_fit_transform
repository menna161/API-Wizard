#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root fit_transform/snippets.json fit_transform"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root fit_transform/test.json fit_transform/test.py"
time python3 src/main/python/similar.py -c fit_transform/snippets.json -d fit_transform/tmpout
time python3 src/main/python/similar.py -d fit_transform/tmpout -f fit_transform/test.json -o fit_transform/output.txt
