#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root nunique/snippets.json nunique"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root nunique/test.json nunique/test.py"
time python3 src/main/python/similar.py -c nunique/snippets.json -d nunique/tmpout
time python3 src/main/python/similar.py -d nunique/tmpout -f nunique/test.json -o nunique/output.txt
