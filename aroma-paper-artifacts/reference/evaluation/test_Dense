#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root Dense/snippets.json Dense"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root Dense/test.json Dense/test.py"
time python3 src/main/python/similar.py -c Dense/snippets.json -d Dense/tmpout
time python3 src/main/python/similar.py -d Dense/tmpout -f Dense/test.json -o Dense/output.txt
