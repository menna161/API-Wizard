#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root cbrt/snippets.json cbrt"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root cbrt/test.json cbrt/test.py"
time python3 src/main/python/similar.py -c cbrt/snippets.json -d cbrt/tmpout
time python3 src/main/python/similar.py -d cbrt/tmpout -f cbrt/test.json -o cbrt/output.txt
