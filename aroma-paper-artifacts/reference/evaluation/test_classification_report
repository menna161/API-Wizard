#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root classification_report/snippets.json classification_report"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root classification_report/test.json classification_report/test.py"
time python3 src/main/python/similar.py -c classification_report/snippets.json -d classification_report/tmpout
time python3 src/main/python/similar.py -d classification_report/tmpout -f classification_report/test.json -o classification_report/output.txt
