#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root Conv2D/snippets.json Conv2D"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root Conv2D/test.json Conv2D/test.py"
time python3 src/main/python/similar.py -c Conv2D/snippets.json -d Conv2D/tmpout
time python3 src/main/python/similar.py -d Conv2D/tmpout -f Conv2D/test.json -o Conv2D/output.txt
