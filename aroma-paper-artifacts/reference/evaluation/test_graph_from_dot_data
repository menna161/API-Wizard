#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root graph_from_dot_data/snippets.json graph_from_dot_data"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root graph_from_dot_data/test.json graph_from_dot_data/test.py"
time python3 src/main/python/similar.py -c graph_from_dot_data/snippets.json -d graph_from_dot_data/tmpout
time python3 src/main/python/similar.py -d graph_from_dot_data/tmpout -f graph_from_dot_data/test.json -o graph_from_dot_data/output.txt
