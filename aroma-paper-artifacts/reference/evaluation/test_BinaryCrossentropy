#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root BinaryCrossentropy/snippets.json BinaryCrossentropy"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root BinaryCrossentropy/test.json BinaryCrossentropy/test.py"
time python3 src/main/python/similar.py -c BinaryCrossentropy/snippets.json -d BinaryCrossentropy/tmpout
time python3 src/main/python/similar.py -d BinaryCrossentropy/tmpout -f BinaryCrossentropy/test.json -o BinaryCrossentropy/output.txt
