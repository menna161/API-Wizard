#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root plt.subplots/snippets.json plt.subplots"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root plt.subplots/test.json plt.subplots/test.py"
time python3 src/main/python/similar.py -c plt.subplots/snippets.json -d plt.subplots/tmpout
time python3 src/main/python/similar.py -d plt.subplots/tmpout -f plt.subplots/test.json -o plt.subplots/output.txt
