#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root drop_duplicates/snippets.json drop_duplicates"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root drop_duplicates/test.json drop_duplicates/test.py"
time python3 src/main/python/similar.py -c drop_duplicates/snippets.json -d drop_duplicates/tmpout
time python3 src/main/python/similar.py -d drop_duplicates/tmpout -f drop_duplicates/test.json -o drop_duplicates/output.txt
