#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root sns.load_dataset/snippets.json sns.load_dataset"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root sns.load_dataset/test.json sns.load_dataset/test.py"
time python3 src/main/python/similar.py -c sns.load_dataset/snippets.json -d sns.load_dataset/tmpout
time python3 src/main/python/similar.py -d sns.load_dataset/tmpout -f sns.load_dataset/test.json -o sns.load_dataset/output.txt
