#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root comb/snippets.json comb"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root comb/test.json comb/test.py"
time python3 src/main/python/similar.py -c comb/snippets.json -d comb/tmpout
time python3 src/main/python/similar.py -d comb/tmpout -f comb/test.json -o comb/output.txt
