#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root requests.get/snippets.json requests.get"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root requests.get/test.json requests.get/test.py"
time python3 src/main/python/similar.py -c requests.get/snippets.json -d requests.get/tmpout
time python3 src/main/python/similar.py -d requests.get/tmpout -f requests.get/test.json -o requests.get/output.txt
