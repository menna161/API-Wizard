#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root requests.post/snippets.json requests.post"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root requests.post/test.json requests.post/test.py"
time python3 src/main/python/similar.py -c requests.post/snippets.json -d requests.post/tmpout
time python3 src/main/python/similar.py -d requests.post/tmpout -f requests.post/test.json -o requests.post/output.txt
