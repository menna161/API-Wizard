#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root requests.request/snippets.json requests.request"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root requests.request/test.json requests.request/test.py"
time python3 src/main/python/similar.py -c requests.request/snippets.json -d requests.request/tmpout
time python3 src/main/python/similar.py -d requests.request/tmpout -f requests.request/test.json -o requests.request/output.txt
