#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root sns.lineplot/snippets.json sns.lineplot"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root sns.lineplot/test.json sns.lineplot/test.py"
time python3 src/main/python/similar.py -c sns.lineplot/snippets.json -d sns.lineplot/tmpout
time python3 src/main/python/similar.py -d sns.lineplot/tmpout -f sns.lineplot/test.json -o sns.lineplot/output.txt
