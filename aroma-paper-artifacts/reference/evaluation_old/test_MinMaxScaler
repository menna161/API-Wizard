#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root MinMaxScaler/snippets.json MinMaxScaler"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root MinMaxScaler/test.json MinMaxScaler/test.py"
time python3 src/main/python/similar.py -c MinMaxScaler/snippets.json -d MinMaxScaler/tmpout
time python3 src/main/python/similar.py -d MinMaxScaler/tmpout -f MinMaxScaler/test.json -o MinMaxScaler/output.txt
