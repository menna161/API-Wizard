#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root requests.patch/snippets.json requests.patch"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root requests.patch/test.json requests.patch/test.py"
time python3 src/main/python/similar.py -c requests.patch/snippets.json -d requests.patch/tmpout
time python3 src/main/python/similar.py -d requests.patch/tmpout -f requests.patch/test.json -o requests.patch/output.txt
