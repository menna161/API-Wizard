#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root cv2.contourArea/snippets.json cv2.contourArea"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root cv2.contourArea/test.json cv2.contourArea/test.py"
time python3 src/main/python/similar.py -c cv2.contourArea/snippets.json -d cv2.contourArea/tmpout
time python3 src/main/python/similar.py -d cv2.contourArea/tmpout -f cv2.contourArea/test.json -o cv2.contourArea/output.txt
