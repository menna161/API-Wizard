#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root tf.pad/snippets.json tf.pad"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root tf.pad/test.json tf.pad/test.py"
time python3 src/main/python/similar.py -c tf.pad/snippets.json -d tf.pad/tmpout
time python3 src/main/python/similar.py -d tf.pad/tmpout -f tf.pad/test.json -o tf.pad/output.txt
