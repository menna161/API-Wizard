#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root Sequential.add/snippets.json Sequential.add"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root Sequential.add/test.json Sequential.add/test.py"
time python3 src/main/python/similar.py -c Sequential.add/snippets.json -d Sequential.add/tmpout
time python3 src/main/python/similar.py -d Sequential.add/tmpout -f Sequential.add/test.json -o Sequential.add/output.txt
