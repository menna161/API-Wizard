#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root LinearRegression/snippets.json LinearRegression"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root LinearRegression/test.json LinearRegression/test.py"
time python3 src/main/python/similar.py -c LinearRegression/snippets.json -d LinearRegression/tmpout
time python3 src/main/python/similar.py -d LinearRegression/tmpout -f LinearRegression/test.json -o LinearRegression/output.txt
