#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root requests.head/snippets.json requests.head"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root requests.head/test.json requests.head/test.py"
time python3 src/main/python/similar.py -c requests.head/snippets.json -d requests.head/tmpout
time python3 src/main/python/similar.py -d requests.head/tmpout -f requests.head/test.json -o requests.head/output.txt
