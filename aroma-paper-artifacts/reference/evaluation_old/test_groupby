#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root groupby/snippets.json groupby"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root groupby/test.json groupby/test.py"
time python3 src/main/python/similar.py -c groupby/snippets.json -d groupby/tmpout
time python3 src/main/python/similar.py -d groupby/tmpout -f groupby/test.json -o groupby/output.txt
