#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root Model.get_layer/snippets.json Model.get_layer"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root Model.get_layer/test.json Model.get_layer/test.py"
time python3 src/main/python/similar.py -c Model.get_layer/snippets.json -d Model.get_layer/tmpout
time python3 src/main/python/similar.py -d Model.get_layer/tmpout -f Model.get_layer/test.json -o Model.get_layer/output.txt
