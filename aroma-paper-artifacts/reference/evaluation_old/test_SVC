#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root SVC/snippets.json SVC"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root SVC/test.json SVC/test.py"
time python3 src/main/python/similar.py -c SVC/snippets.json -d SVC/tmpout
time python3 src/main/python/similar.py -d SVC/tmpout -f SVC/test.json -o SVC/output.txt
