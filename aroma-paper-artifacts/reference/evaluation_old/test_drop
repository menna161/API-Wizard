#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root drop/snippets.json drop"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root drop/test.json drop/test.py"
time python3 src/main/python/similar.py -c drop/snippets.json -d drop/tmpout
time python3 src/main/python/similar.py -d drop/tmpout -f drop/test.json -o drop/output.txt
