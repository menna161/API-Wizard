#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root prettify/snippets.json prettify"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root prettify/test.json prettify/test.py"
time python3 src/main/python/similar.py -c prettify/snippets.json -d prettify/tmpout
time python3 src/main/python/similar.py -d prettify/tmpout -f prettify/test.json -o prettify/output.txt
