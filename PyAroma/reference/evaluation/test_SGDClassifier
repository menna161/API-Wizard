#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root SGDClassifier/snippets.json SGDClassifier"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root SGDClassifier/test.json SGDClassifier/test.py"
time python3 src/main/python/similar.py -c SGDClassifier/snippets.json -d SGDClassifier/tmpout
time python3 src/main/python/similar.py -d SGDClassifier/tmpout -f SGDClassifier/test.json -o SGDClassifier/output.txt
