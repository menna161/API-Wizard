#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root confusion_matrix/snippets.json confusion_matrix"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root confusion_matrix/test.json confusion_matrix/test.py"
time python3 src/main/python/similar.py -c confusion_matrix/snippets.json -d confusion_matrix/tmpout
time python3 src/main/python/similar.py -d confusion_matrix/tmpout -f confusion_matrix/test.json -o confusion_matrix/output.txt
