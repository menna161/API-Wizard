#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root soup.get_text/snippets.json soup.get_text"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root soup.get_text/test.json soup.get_text/test.py"
time python3 src/main/python/similar.py -c soup.get_text/snippets.json -d soup.get_text/tmpout
time python3 src/main/python/similar.py -d soup.get_text/tmpout -f soup.get_text/test.json -o soup.get_text/output.txt
