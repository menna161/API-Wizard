#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root np.arange/snippets.json np.arange"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root np.arange/test.json np.arange/test.py"
time python3 src/main/python/similar.py -c np.arange/snippets.json -d np.arange/tmpout
time python3 src/main/python/similar.py -d np.arange/tmpout -f np.arange/test.json -o np.arange/output.txt
