#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root cv2.setMouseCallback/snippets.json cv2.setMouseCallback"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root cv2.setMouseCallback/test.json cv2.setMouseCallback/test.py"
time python3 src/main/python/similar.py -c cv2.setMouseCallback/snippets.json -d cv2.setMouseCallback/tmpout
time python3 src/main/python/similar.py -d cv2.setMouseCallback/tmpout -f cv2.setMouseCallback/test.json -o cv2.setMouseCallback/output.txt
