#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root np.linspace/snippets.json np.linspace"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root np.linspace/test.json np.linspace/test.py"
time python3 src/main/python/similar.py -c np.linspace/snippets.json -d np.linspace/tmpout
time python3 src/main/python/similar.py -d np.linspace/tmpout -f np.linspace/test.json -o np.linspace/output.txt
