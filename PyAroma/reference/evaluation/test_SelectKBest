#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root SelectKBest/snippets.json SelectKBest"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root SelectKBest/test.json SelectKBest/test.py"
time python3 src/main/python/similar.py -c SelectKBest/snippets.json -d SelectKBest/tmpout
time python3 src/main/python/similar.py -d SelectKBest/tmpout -f SelectKBest/test.json -o SelectKBest/output.txt
