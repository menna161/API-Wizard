#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root perm/snippets.json perm"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root perm/test.json perm/test.py"
time python3 src/main/python/similar.py -c perm/snippets.json -d perm/tmpout
time python3 src/main/python/similar.py -d perm/tmpout -f perm/test.json -o perm/output.txt
