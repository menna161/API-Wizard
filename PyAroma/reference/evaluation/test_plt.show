#!/bin/bash
cd ..
mvn clean package
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root plt.show/snippets.json plt.show"
time mvn exec:java -Dexec.mainClass=ConvertJava -Dexec.args="root plt.show/test.json plt.show/test.py"
time python3 src/main/python/similar.py -c plt.show/snippets.json -d plt.show/tmpout
time python3 src/main/python/similar.py -d plt.show/tmpout -f plt.show/test.json -o plt.show/output.txt
